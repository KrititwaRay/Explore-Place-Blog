<h1 class="pb-4" >Search Results</h1>

<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
    <% if( typeof place !=='undefined' && place.length>0){%>
        <% place.forEach(function(place,index){%>
       
           
          <a href="/places/<%= place._id%>" class="col text-center category__link">
              <div class="category__img category__img__large  shadow" >
                  <img src="/uploads/<%= place.image%>"   alt="<%= place.name%>" >
             </div>
             <div class="pt-1"> <%= place.name%></div>
          </a>
          
          <%}) %>
      
      <%}else{%>
         
           <p>No items Found</p>
         <%} %>
 </div>